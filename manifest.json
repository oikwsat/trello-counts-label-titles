{
  "manifest_version": 2,
  "name": "LabelCounter for Trello",
  "description": "Counts the Card label titles on trello.com",
  "version": "1.0",
  "icons" : {
    "16":  "icons/icon16.png",
    "48":  "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://trello.com/b/*"
      ],
      "js": [
        "js/injector.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "js/embeded.js"
  ],
  "permissions": [
    "https://trello.com/b/*"
  ]
}
